{% liquid
  assign heading = section.settings.heading
  assign heading_size = section.settings.heading_size
  assign heading_weight = section.settings.heading_weight
  assign heading_color = section.settings.heading_color
  assign heading_font = section.settings.heading_font

  assign heading_size_mobile = section.settings.heading_size_mobile

  assign slider_color = section.settings.slider_color
  assign slider_per_view = section.settings.slider_per_view
  assign slider_per_view_mobile = section.settings.slider_per_view_mobile
  assign slider_space_between = section.settings.slider_space_between
  assign slider_space_between_mobile = section.settings.slider_space_between_mobile
  assign enable_autoplay = section.settings.enable_autoplay
  assign slider_speed = section.settings.slider_speed
  assign enable_loop = section.settings.enable_loop
  assign enable_pagination = section.settings.enable_pagination
  assign enable_navigation = section.settings.enable_navigation

  assign card_border_radius = section.settings.card_border_radius
  assign card_background = section.settings.card_background
  assign card_text_color = section.settings.card_text_color
  assign card_border_width = section.settings.card_border_width
  assign card_border_color = section.settings.card_border_color
  assign card_border_style = section.settings.card_border_style
  assign card_shadow = section.settings.card_shadow

  assign layout_margin_top = section.settings.layout_margin_top
  assign layout_margin_bottom = section.settings.layout_margin_bottom
  assign layout_background = section.settings.layout_background
%}
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css">
<style>
  .pc-section-{{section.id}} {
      margin-top: {{ layout_margin_top }}px;
      margin-bottom: {{ layout_margin_bottom }}px;
      overflow: hidden;
  }

  .pc-section-{{section.id}} .pc-section-{{section.id}}-settings {
      margin: 0 auto;
      padding-top: 27px;
      padding-bottom: 27px;
      padding-left: 1.5rem;
      padding-right: 1.5rem;
  }

  .pc-section-{{section.id}} .pc-testimonials-heading-{{section.id}} {
      text-align: center;
  }

  .pc-section-{{section.id}} .pc-testimonials-heading-{{section.id}} * {
      margin: 0;
      font-weight: {{ heading_weight }};
      font-size: {{ heading_size_mobile }}px;
      color: {{ heading_color }};
      line-height: 130%;
      font-family: {{ heading_font.family }}, {% if heading_font.fallback == blank %} sans-serif {% else %} {{ heading_font.fallback }} {% endif %} !important;
      text-transform: unset;
      text-decoration: none;
  }

  .pc-section-{{section.id}} .pc-testimonials-slider-body-{{section.id}} {
      display: flex;
      margin-top: 10px;
      align-items: center;
      gap: 15px;
  }

  .pc-section-{{section.id}} .pc-testimonials-slider-{{section.id}} {
      flex: 1 1 0 !important;
      padding: 20px 0px !important;
      margin-left: -1.5rem !important;
      margin-right: -1.5rem !important;
      padding-left: 1.5rem !important;
      padding-right: 1.5rem !important;
  }

  .pc-section-{{section.id}} .pc-testimonials-slide-{{section.id}} {
      box-sizing: border-box !important;
      padding: 12px 16px !important;
      border: {{ card_border_width }}px {{ card_border_style }} {{ card_border_color }} ;
      border-radius: {{ card_border_radius }}px;
      background-color: {{ card_background }};
  }

  .pc-section-{{section.id}} .pc-testimonials-author-{{section.id}} {
      display: flex;
      align-items: center;
      justify-content: start;
      gap: 10px;
  }

  .pc-section-{{section.id}} .pc-testimonials-author-image-{{section.id}} {
      flex-shrink: 0;
      width: 40px;
      border-radius: 100px;
      overflow: hidden;
      aspect-ratio: 1/1;
  }

  .pc-section-{{section.id}} .pc-testimonials-author-image-{{section.id}} img,
  .pc-section-{{section.id}} .pc-testimonials-author-image-{{section.id}} svg {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }

  .pc-section-{{section.id}} .pc-testimonials-author-image-{{section.id}} svg {
      background-color: #AFAFAF;
  }

  .pc-section-{{section.id}} .pc-testimonials-author-text-{{section.id}} {
      margin: 0;
      font-weight: 700;
      font-size: 15px;
      color: {{ card_text_color }};
      line-height: 130%;
      text-transform: unset;
      word-wrap: break-word;
      text-align: left;
  }

  .pc-section-{{section.id}} .pc-testimonials-author-time-{{section.id}} {
      margin: 0;
      margin-top: 2px;
      font-size: 13px;
      color: {{ card_text_color }};
      line-height: 130%;
      text-transform: unset;
      word-wrap: break-word;
      text-align: left;
  }

  .pc-section-{{section.id}} .pc-testimonials-text-{{section.id}} {
      margin-top: 10px;
      text-align: left;
  }

  .pc-section-{{section.id}} .pc-testimonials-text-{{section.id}} * {
      margin: 0;
      font-size: 13px;
      color: {{ card_text_color}};
      line-height: 170%;
      text-transform: unset;
      text-decoration: none;
  }

  .pc-section-{{section.id}} .pc-testimonials-image-{{section.id}} {
      width: 100%;
      margin-top: 10px;
      margin-left: 0px;
      margin-right: 0px;
      border-radius: 0px;
      overflow: hidden;
  }

  .pc-section-{{section.id}} .pc-testimonials-image-{{section.id}} img,
  .pc-section-{{section.id}} .pc-testimonials-image-{{section.id}} svg {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }

  .pc-section-{{section.id}} .pc-testimonials-comments-{{section.id}} {
      display: flex;
      align-items: center;
      justify-content: space-between;
      flex-wrap: wrap;
      margin-top: 12px;
  }

  .pc-section-{{section.id}} .pc-testimonials-count-comments-{{section.id}} {
      display: flex;
      align-items: center;
      gap: 5px;
  }

  .pc-section-{{section.id}} .pc-testimonials-count-comments-icons-{{section.id}} {
      display: flex;
      align-items: center;
  }

  .pc-section-{{section.id}} .pc-testimonials-count-comments-icon-{{section.id}} {
      width: 18px;
      height: 18px;
  }

  .pc-section-{{section.id}} .pc-testimonials-count-comments-icon-{{section.id}} img,
  .pc-section-{{section.id}} .pc-testimonials-count-comments-icon-{{section.id}} svg {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }

  .pc-section-{{section.id}} .pc-testimonials-count-comment-{{section.id}} {
      margin: 0;
      width: 100%;
      text-align: left;
      font-size: 12px;
      color: {{ card_text_color }};
      line-height: 130%;
      text-transform: unset;
      word-wrap: break-word;
  }

  .pc-section-{{section.id}} .pc-testimonials-comment-{{section.id}} {
      margin: 0;
      font-size: 12px;
      color: {{ card_text_color }};
      line-height: 130%;
      text-transform: unset;
      word-wrap: break-word;
  }

  .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}},
  .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}} {
      display: flex;
      flex-shrink: 0;
      width: 38px;
      height: 38px;
      border: 0px;
      border: 0px solid #c7c7c7;
      border-radius: 100px;
      background: transparent;
      background: {{ slider_color }};
      align-items: center;
      justify-content: center;
      transition: all 0.3s ease 0s;
      cursor: pointer;
      -webkit-appearance: none;
      /* box-shadow: 0px 0px 10px 0px rgba(0,0,0,.15); */
  }

  .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}} .pc-animate-icon,
  .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}} .pc-animate-icon {
      pointer-events: none;
      place-items: center;
      display: grid;
  }

  .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}} .pc-animate-icon:before,
  .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}} .pc-animate-icon:before {
      content: "";
      grid-area: 1 / -1;
      transition: all .2s ease;
      width: 8px;
      height: 8px;
      transform: rotate(calc(1 * 45deg));
      box-sizing: border-box;
      border-style: solid;
      border: 0px;
      border-right: 2px solid currentColor;
      border-top: 2px solid currentColor;
      position: relative;
      left: -1px;
  }

  .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}} .pc-animate-icon:before {
      left: unset;
      right: -1px;
      border-right: 0px;
      border-top: 0px;
      border-bottom: 2px solid currentColor;
      border-left: 2px solid currentColor;
  }

  .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}} .pc-animate-icon:after,
  .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}} .pc-animate-icon:after {
      content: "";
      grid-area: 1 / -1;
      transition: all .2s ease;
      width: 7.7px;
      height: 1.6px;
      transform-origin: center;
      opacity: 0;
      background: currentColor;
      transform: scaleX(.5);
  }

  .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}} span,
  .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}} span {
      color: #ffffff;
  }

  .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}}.swiper-button-disabled,
  .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}}.swiper-button-disabled {
      opacity: 0.5;
  }

  .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}}.swiper-button-lock,
  .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}}.swiper-button-lock {
      opacity: 0;
      pointer-events: none;
  }

  .pc-section-{{section.id}} .pc-testimonials-pagination-{{section.id}} {
      position: static !important;
      margin: 0px auto !important;
      margin-top: 16px !important;
      width: 100% !important;
      max-width: fit-content;
      justify-content: center;
      align-items: center;
  }

  .pc-section-{{section.id}} .pc-testimonials-pagination-{{section.id}} .swiper-pagination-bullet {
      width: 6px !important;
      height: 6px !important;
      border: 0px !important;
      background-color: {{ slider_color }}!important;
      opacity: 0.4 !important;
      transition: all 0.25s ease 0s;
  }

  .pc-section-{{section.id}} .pc-testimonials-pagination-{{section.id}} .swiper-pagination-bullet.swiper-pagination-bullet-active {
      transform: scale(1.3) !important;
      opacity: 1 !important;
      transition: all 0.25s ease 0s;
  }

  .pc-section-{{section.id}} .pc-testimonials-buttons-{{section.id}} {
      display: flex;
      align-items: center;
      justify-content: space-between;
      flex-wrap: wrap;
      gap: 10px;
      border-top: 1px solid #a4b0be;
      margin-top: 10px;
      padding-top: 10px;
  }

  .pc-section-{{section.id}} .pc-testimonials-button-{{section.id}} {
      display: flex;
      align-items: center;
      gap: 5px;
  }

  .pc-section-{{section.id}} .pc-testimonials-button-icon-{{section.id}} {
      width: 16px;
      height: 16px;
  }

  .pc-section-{{section.id}} .pc-testimonials-button-icon-{{section.id}} svg path {
      fill: #000000;
  }

  .pc-section-{{section.id}} .pc-testimonials-button-icon-{{section.id}} svg,
  .pc-section-{{section.id}} .pc-testimonials-button-icon-{{section.id}} img {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }

  .pc-section-{{section.id}} .pc-testimonials-button-text-{{section.id}} {
      margin: 0;
      font-size: 12px;
      color: {{ card_text_color }};
      line-height: 130%;
      text-transform: unset;
      word-wrap: break-word;
  }

  @media(min-width: 1024px) {

      .pc-section-{{section.id}} .pc-section-{{section.id}} {
          margin-top: {{ layout_margin_top }}px;
          margin-bottom: {{ layout_margin_bottom }}px;
      }

      .pc-section-{{section.id}} .pc-section-{{section.id}}-settings {
          padding: 0 5rem;
          padding-top: 36px;
          padding-bottom: 36px;
          padding-left: 1rem;
          padding-right: 1rem;
      }

      .pc-section-{{section.id}} .pc-testimonials-heading-{{section.id}} {
          text-align: center;
      }

      .pc-section-{{section.id}} .pc-testimonials-heading-{{section.id}} * {
          font-size: {{ heading_size }}px;
      }

      .pc-section-{{section.id}} .pc-testimonials-slider-body-{{section.id}} {
          margin-top: 10px;
      }

      .pc-section-{{section.id}} .pc-testimonials-slider-{{section.id}} {
          margin-left: -0rem !important;
          margin-right: -0rem !important;
          padding-left: 20px !important;
          padding-right: 20px !important;
      }

      .pc-section-{{section.id}} .pc-testimonials-slide-{{section.id}} {
          padding: 12px 16px !important;
      }

      .pc-section-{{section.id}} .pc-testimonials-author-{{section.id}} {
          justify-content: start;
          gap: 10px;
      }

      .pc-section-{{section.id}} .pc-testimonials-author-image-{{section.id}} {
          width: 40px;
      }

      .pc-section-{{section.id}} .pc-testimonials-author-text-{{section.id}} {
          font-size: 15px;
          text-align: left;
      }

      .pc-section-{{section.id}} .pc-testimonials-author-time-{{section.id}} {
          margin-top: 2px;
          font-size: 13px;
          text-align: left;
      }

      .pc-section-{{section.id}} .pc-testimonials-text-{{section.id}} {
          margin-top: 10px;
          text-align: left;
      }

      .pc-section-{{section.id}} .pc-testimonials-text-{{section.id}} * {
          font-size: 14px;
          margin: 0;
        }

      .pc-section-{{section.id}} .pc-testimonials-image-{{section.id}} {
          margin-top: 10px;
      }

      .pc-section-{{section.id}} .pc-testimonials-comments-{{section.id}} {
          margin-top: 12px;
      }

      .pc-section-{{section.id}} .pc-testimonials-count-comment-{{section.id}} {
          font-size: 13px;
      }

      .pc-section-{{section.id}} .pc-testimonials-comment-{{section.id}} {
          font-size: 13px;
      }

      .pc-section-{{section.id}} .pc-testimonials-count-comments-icon-{{section.id}} {
          width: 18px;
          height: 18px;
      }

      .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}},
      .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}} {
          width: 48px;
          height: 48px;
          transition: all 0.25s ease 0s;
      }

      .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}} .pc-animate-icon:before,
      .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}} .pc-animate-icon:before {
          width: 10px;
          height: 10px;
      }

      .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}} .pc-animate-icon:after,
      .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}} .pc-animate-icon:after {
          width: 9.7px;
      }

      .pc-section-{{section.id}} .pc-testimonials-pagination-{{section.id}} {
          margin-top: 20px !important;
      }

      .pc-section-{{section.id}} .pc-testimonials-pagination-{{section.id}} .swiper-pagination-bullet {
          width: 6px !important;
          height: 6px !important;
      }

      .pc-section-{{section.id}} .pc-testimonials-buttons-{{section.id}} {
          margin-top: 10px;
      }

      .pc-section-{{section.id}} .pc-testimonials-button-text-{{section.id}} {
          font-size: 13px;
      }

      .pc-section-{{section.id}} .pc-testimonials-button-icon-{{section.id}} {
          width: 16px;
          height: 16px;
      }
  }


  .pc-section-{{section.id}} .pc-section-{{section.id}}-settings {
      max-width: 1400px;
  }


  @media(min-width: 1024px) {
      .pc-section-{{section.id}} .pc-testimonials-author-{{section.id}} {
          flex-direction: row;
      }
  }


  .pc-section-{{section.id}} .pc-testimonials-image-{{section.id}} {
      aspect-ratio: 12/8;
  }


  @media(min-width: 1024px) {
      .pc-section-{{section.id}} .pc-testimonials-image-{{section.id}} {
          aspect-ratio: 12/8;
      }
  }


  .pc-section-{{section.id}} .pc-testimonials-image-{{section.id}} {
      width: auto;
      margin-left: -16px;
      margin-right: -16px;
  }

  @media(min-width: 1024px) {
      .pc-section-{{section.id}} .pc-testimonials-image-{{section.id}} {
          margin-left: -16px;
          margin-right: -16px;
      }
  }

  {% if card_shadow == true %}
  .pc-section-{{section.id}} .pc-testimonials-slide-{{section.id}} {
      box-shadow: 2px 2px 6px rgba(0, 0, 0, 0.149);
  }
  {% endif %}


  .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}}:hover,
  .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}}:hover {
      transition: all 0.3s ease 0s;
      border: 0px solid #c7c7c7;
      background: {{ slider_color }};
  }

  .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}}:hover .pc-animate-icon,
  .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}}:hover .pc-animate-icon {
      color: #ffffff;
  }


  .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}},
  .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}} {
      display: none;
  }


  @media(min-width: 1024px) {

      .pc-section-{{section.id}} .pc-testimonials-btn-prev-{{section.id}},
      .pc-section-{{section.id}} .pc-testimonials-btn-next-{{section.id}} {
          display: flex;
      }
  }


  .pc-section-{{section.id}} .pc-testimonials-pagination-{{section.id}} {
      display: flex !important;
  }


  @media(min-width: 1024px) {
      .pc-section-{{section.id}} .pc-testimonials-pagination-{{section.id}} {
          display: flex !important;
      }
  }


  .pc-section-{{section.id}} .pc-testimonials-slider-{{section.id}}.preview .swiper-wrapper {
      display: flex;
      align-items: center;
      justify-content: space-between;
      overflow: hidden;
      padding: 15px;
  }

  .pc-section-{{section.id}} .pc-testimonials-slider-{{section.id}}.preview .pc-section-{{section.id}} .pc-testimonials-slide-{{section.id}} {
      width: 100%;
  }

  .pc-section-{{section.id}} .pc-testimonials-slider-{{section.id}}.preview .pc-section-{{section.id}} .pc-testimonials-slide-{{section.id}}:not(:first-child) {
      margin-left: 24px;
  }

  @media(min-width: 1024px) {
      .pc-section-{{section.id}} .pc-testimonials-slider-{{section.id}}.preview .pc-section-{{section.id}} .pc-testimonials-slide-{{section.id}}:not(:first-child) {
          margin-left: 47px;
      }

      .pc-section-{{section.id}} .pc-testimonials-slider-{{section.id}}.preview .pc-section-{{section.id}} .pc-testimonials-slide-{{section.id}} {
          width: calc(100% / 3 - (32px));
      }
  }
</style>

<div
  class="pc-section-{{section.id}} pc-testimonials-{{section.id}}"
  style="background-color:{{ layout_background }}; background-image: ;"
>
  <div class="pc-section-{{section.id}}-settings">
    {% if heading != blank %}
      <div class="pc-testimonials-heading-{{section.id}}">
        <h2>{{ heading }}</h2>
      </div>
    {% endif %}

    <div class="pc-testimonials-slider-body-{{section.id}}">
    {% if enable_navigation == true %}  
    <button class="pc-testimonials-btn-prev-{{section.id}}">
        <span class="pc-animate-icon"></span>
      </button>
{% endif %}
      <div
        class="pc-testimonials-slider-{{section.id}} swiper "
      >
        <div class="swiper-wrapper">
            {% for block in section.blocks %}
          <div class="pc-testimonials-slide-{{section.id}} swiper-slide">
            <div class="pc-testimonials-author-{{section.id}}">
              <div
                class="pc-testimonials-author-image-{{section.id}}"
              >
              {% if block.settings.image != blank %}
                <img
                  src="{{ block.settings.image |  img_url: 'master' }}"
                  alt=""
                >
                {% else %}
                {{ 'image' |  placeholder_svg_tag: 'placeholder_svg' }}
              {% endif %}
              </div>
              <div
                class="pc-testimonials-author-content-{{section.id}}"
              >
                <p
                  class="pc-testimonials-author-text-{{section.id}}"
                >
                  {{ block.settings.name }}
                </p>

                <p
                  class="pc-testimonials-author-time-{{section.id}}"
                >
                  {{ block.settings.date }}
                </p>
              </div>
            </div>

            <div class="pc-testimonials-text-{{section.id}}">
              {{block.settings.content}}
            </div>

            <div class="pc-testimonials-image-{{section.id}}">
            {% if block.settings.post_media != blank %}  
            <img
                src="{{ block.settings.post_media | img_url: 'master' }}"
                alt=""
              >
              {% else %}
                {{ 'collection-4' | placeholder_svg_tag: 'placeholder-svg' }}
            {% endif %}
            </div>

            <div class="pc-testimonials-comments-{{section.id}}">
              <div
                class="pc-testimonials-count-comments-{{section.id}}"
              >
                <div
                  class="pc-testimonials-count-comments-icons-{{section.id}}"
                >
                {% if block.settings.enable_likes == true %}
                  <div
                    class="pc-testimonials-count-comments-icon-{{section.id}}"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      width="18"
                      height="18"
                      fill="none"
                      viewBox="0 0 16 16"
                    >
                      <defs>
                          <linearGradient id="gradient-column_YpyKXV0" x1="8" x2="8" y2="16"
                              gradientUnits="userSpaceOnUse">
                              <stop stop-color="#18AFFF"></stop>
                              <stop offset="1" stop-color="#0062DF"></stop>
                          </linearGradient>
                      </defs>
                      <path fill="url(#gradient-column_YpyKXV0)"
