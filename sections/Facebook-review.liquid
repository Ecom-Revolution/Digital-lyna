<script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>

<script>
  function initTestimonials20() {
      document.querySelector('.pc-testimonials-slider-{{section.id}}').classList.remove('preview');
      new Swiper(".pc-testimonials-slider-{{section.id}}", {
          speed: {{ slider_speed }},
          {% if enable_loop == true %}
          loop: true,
          {% endif %}
          {% if enable_autoplay == true %}
            autoplay: {
                delay: {{ slider_speed }}
            }
            {% endif %}
          navigation: {
              nextEl: '.pc-testimonials-btn-next-{{section.id}}',
              prevEl: '.pc-testimonials-btn-prev-{{section.id}}',
          },
          pagination: {
              el: '.pc-testimonials-pagination-{{section.id}}',
              type: "bullets",
              clickable: true
          },
          breakpoints: {
              320: {
                  spaceBetween: {{ slider_space_between_mobile }},
                  slidesPerView: {{ slider_per_view_mobile }}.3,
              },
              768: {
                  spaceBetween: {{ slider_space_between_mobile }},
                  slidesPerView: {{ slider_per_view_mobile }}.5,
              },
              1024: {
                  spaceBetween: {{ slider_space_between }},
                  slidesPerView: {{ slider_per_view }},
              }
          }
      });
  }

  document.addEventListener('DOMContentLoaded', initTestimonials20);
</script>
